    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "glob": "^11.0.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.16.2",
    "typescript": "5.9.2"
  }
