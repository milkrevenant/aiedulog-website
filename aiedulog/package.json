{
  "name": "aiedulog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "build:analyze": "ANALYZE=true next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules",
    "pre-commit": "npm run type-check && npm run lint",

    "amplify:simulate": "NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 CI=true npm run build",
    "health:check": "node scripts/system-health-check.js",
    "identity:fix": "node scripts/fix-identity-system.js",
    "identity:test": "node scripts/test-identity-function.js",
    "cache:clear": "node scripts/clear-dev-cache.js"
  },
  "dependencies": {
